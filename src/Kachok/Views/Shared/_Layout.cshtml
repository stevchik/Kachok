<!DOCTYPE html>

<html ng-app="kachokApp" lang="en">

    <head>
        <meta name="viewport" content="width=device-width" />
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="EI=edge" />
        <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no" />

        <!-- Polyfill(s) for older browsers -->
        <script src="~/lib/core-js/client/shim.min.js"></script>
        <script src="~/lib/zone.js/dist/zone.js"></script>
        <script src="~/lib/reflect-metadata/Reflect.js"></script>
        <script src="~/lib/systemjs/dist/system.src.js"></script>
        <script src="systemjs.config.js"></script>
        <script>
            System.import('app').catch(function (err) { console.error(err); });
        </script>
        <title>Kachok - @ViewBag.Title</title>

        <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">

        <link href="~/css/app.css" rel="stylesheet" />
        <link href="~/css/site.css" rel="stylesheet" />
        <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
        <link href="~/css/ie10-viewport-bug-workaround.css" rel="stylesheet">
    </head>

    <body>
        <header>
            <div class="navbar navbar-standard navbar-static-top">
                <div class="navbar-inner">
                    <div class="container">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <a class="navbar-brand" href="#">Kachok Club</a>
                        </div>

                        <nav class="navbar-collapse collapse" id="navbar">
                            <ul class="nav navbar-nav navbar-right">
                                <li><a href="#">Exercises</a></li>
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Workout<span class="caret"></span></a>
                                    <ul class="dropdown-menu">
                                        <li><a href="#">Log</a></li>
                                        <li><a href="#">Plan</a></li>
                                        <li role="separator" class="divider"></li>
                                        <li class="dropdown-header">History</li>
                                        <li><a href="#">Completed Logs</a></li>
                                    </ul>
                                </li>
                                <li><a href="">Progress</a></li>
                                <li><a href="">Resources</a></li>
                                <li><a href="">About</a></li>
                            </ul>
                        </nav><!--/.nav-collapse -->
                    </div>
                </div>
            </div>
        </header>
        <div class="container">

        <div id="breadcrumb">
            <ol class="breadcrumb">
                <li><a href="#">Home</a></li>
                <li><a href="#">Library</a></li>
                <li class="active">Data</li>
            </ol>
        </div>
            <my-app>Loading...</my-app>
            @RenderBody()

        </div><!-- /.container -->

        <footer class="footer">
            <div class="container text-center">
                <div class="row">
                    <div class="col-xs-12 col-sm-2">
                        <ul class="nav nav-pills nav-stacked">
                            <li><a href="#">Exercises</a></li>
                        </ul>
                    </div>
                    <div class="col-xs-12 col-sm-2">
                        <ul class="nav nav-pills nav-stacked">
                            <li><a href="#">Workout Log</a></li>
                            <li><a href="#">Workout Plan</a></li>
                            <li><a href="#">Completed Logs</a></li>
                        </ul>
                    </div>
                    <div class="col-xs-12 col-sm-2">
                        <ul class="nav nav-pills nav-stacked">
                            <li><a href="#">Progress</a></li>
                        </ul>
                    </div>
                    <div class="col-xs-12 col-sm-2">
                        <ul class="nav nav-pills nav-stacked">
                            <li><a href="#">Resources</a></li>
                        </ul>
                    </div>
                    <div class="col-xs-12 col-sm-2">
                        <ul class="nav nav-pills nav-stacked">
                            <li><a href="#">About</a></li>
                        </ul>
                    </div>
                    <div class="col-xs-12 col-sm-2">
                        <ul class="nav nav-pills nav-stacked">
                            <li><a href="#">Back to top</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </footer>

        @*<script type="text/javascript" src="~/scripts/compile/controllers/sitecontroller.js"></script>
            <script type="text/javascript" src="~/scripts/compile/boot.js"></script>*@

        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/bootstrap.min.js"></script>
        <script src="~/scripts/js/ie10-viewport-bug-workaround.js"></script>

        @RenderSection("Scripts", false)

    </body>     

</html>