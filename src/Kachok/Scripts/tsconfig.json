{
  "compilerOptions": {
    "removeComments": false,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "sourceMap": true,
    "target": "es5", //,
    "emitDecoratorMetadata": true,
    "module": "system",
    "moduleResolution": "node",
    "suppressImplicitAnyIndexErrors": true,
    "experimentalDecorators": true //,
    //"outDir": "../wwwroot/scripts/compiled/app"
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules",
    "wwwroot"
  ]

}